<h1>Sign In</h1>

<form action="<%= sessions_url %>" method="post">
  <%= auth_token %>

  <label for="email">
    Email:
    <input id="email" type="text" name="user[email]">
  </label>

  <br>
  <br>

  <label for="password">
    Password:
    <input id="password" type="password" name="user[password]">
  </label>

  <br>
  <br>

  <input type="submit" value="Sign In">
</form>
