<h1>Sign  Up</h1>

<form action="/users" method="post">
  <%= auth_token %>
  <label for="email">
    Email:
    <input id="email" type="text" name="user[email]">
  </label>

  <br>
  <br>

  <label for="password">
    Password:
    <input id="password" type="password" name="user[password]">
  </label>

  <br>
  <br>

  <input type="submit" value="Sign up">
</form>
